{
        "name": "paysera-backend",
        "version": "1.0.0",
        "description": "express+prisma backend for paysera timekeeping system",
        "main": "index.js",
        "scripts": {
                "studio": " dotenv -e .env.development prisma studio",
                "seed": "dotenv -e .env.development ts-node ./src/config/seed.ts",
                "migrate:test": "dotenv -e .env.test  prisma migrate reset",
                "migrate": "dotenv -e .env.development prisma migrate dev",
                "test": "dotenv -e .env.test -- jest",
                "dev": "dotenv -e .env.development ts-node-dev ./src/index.ts",
                "start": "node dist/src/index.js",
                "clean": "rm -rf dist",
                "build": "npm run clean && tsc",
                "copy-files": "cpy 'src/docs/**' dist/src/docs --parents",
                "postbuild": "npm run copy-files",
                "ts.check": "tsc --project tsconfig.json",
                "add-build": "git add dist",
                "secret": "ts-node ./src/secret.ts",
                "postinstall": "prisma generate",
                "sample": "ts-node ./src/sample.ts"
        },
        "pre-commit": [
                "ts.check",
                "build",
                "add-build"
        ],
        "keywords": [],
        "author": "",
        "license": "ISC",
        "dependencies": {
                "@amplitude/ua-parser-js": "^0.7.33",
                "@date-fns/tz": "^1.2.0",
                "@date-fns/utc": "^2.1.0",
                "@prisma/client": "^6.2.1",
                "@types/date-fns": "^2.6.3",
                "@types/jest": "^29.5.12",
                "@types/ua-parser-js": "^0.7.39",
                "bcryptjs": "^2.4.3",
                "compression": "^1.7.4",
                "cookie-parser": "^1.4.6",
                "cors": "^2.8.5",
                "cpy-cli": "^5.0.0",
                "csurf": "^1.11.0",
                "date-fns": "^4.1.0",
                "dotenv": "^16.4.5",
                "dotenv-cli": "^8.0.0",
                "express": "^4.19.2",
                "express-rate-limit": "^7.4.0",
                "helmet": "^8.0.0",
                "hpp": "^0.2.3",
                "jsonwebtoken": "^9.0.2",
                "pino": "^9.3.2",
                "pino-http": "^10.4.0",
                "pino-pretty": "^13.0.0",
                "response-time": "^2.3.2",
                "swagger-ui-dist": "^5.17.14",
                "swagger-ui-express": "^5.0.1",
                "ua-parser-js": "^2.0.0",
                "yamljs": "^0.3.0",
                "zod": "^3.23.8"
        },
        "devDependencies": {
                "@types/bcryptjs": "^2.4.6",
                "@types/compression": "^1.7.5",
                "@types/cookie-parser": "^1.4.7",
                "@types/cors": "^2.8.17",
                "@types/csurf": "^1.11.5",
                "@types/dotenv": "^8.2.3",
                "@types/express": "^4.17.21",
                "@types/express-rate-limit": "^6.0.2",
                "@types/hpp": "^0.2.6",
                "@types/jsonwebtoken": "^9.0.8",
                "@types/module-alias": "^2.0.4",
                "@types/morgan": "^1.9.9",
                "@types/node": "^22.10.10",
                "@types/supertest": "^6.0.2",
                "@types/swagger-ui-dist": "^3.30.5",
                "@types/swagger-ui-express": "^4.1.6",
                "@types/yamljs": "^0.2.34",
                "prisma": "^6.2.1",
                "supertest": "^7.0.0",
                "ts-jest": "^29.2.5",
                "ts-node": "^10.9.2",
                "ts-node-dev": "^2.0.0",
                "typescript": "^5.7.3"
        }
}
